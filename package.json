  },
  "packageManager": "yarn@3.6.0",
  "workspaces": [
    "main",
    "packages/*"
  ],
  "dependencies": {
    "@icewhale/casaos-appmanagement-openapi": "latest"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.8.0",
    "turbo": "1.10.7"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
