{
  "name": "casaos",
  "version": "v0.4.2",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "dev": "turbo run serve",
    "dev:noCache": "turbo run serve --force --cache-dir ./.turbo/cache",
